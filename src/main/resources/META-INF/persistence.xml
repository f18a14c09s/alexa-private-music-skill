<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="alexa-music-data" transaction-type="RESOURCE_LOCAL">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>f18a14c09s.integration.alexa.music.catalog.data.AbstractCatalog</class>
        <class>f18a14c09s.integration.alexa.music.catalog.data.MusicAlbumCatalog</class>
        <class>f18a14c09s.integration.alexa.music.catalog.data.MusicRecordingCatalog</class>
        <class>f18a14c09s.integration.alexa.music.catalog.data.MusicGroupCatalog</class>
        <class>f18a14c09s.integration.alexa.music.entities.BaseEntity</class>
        <class>f18a14c09s.integration.alexa.music.entities.ArtistReference</class>
        <class>f18a14c09s.integration.alexa.music.entities.Artist</class>
        <class>f18a14c09s.integration.alexa.music.entities.AlbumReference</class>
        <class>f18a14c09s.integration.alexa.music.entities.Album</class>
        <class>f18a14c09s.integration.alexa.music.entities.Track</class>
        <class>f18a14c09s.integration.alexa.music.entities.Popularity</class>
        <class>f18a14c09s.integration.alexa.music.entities.EntityName</class>
        <class>f18a14c09s.integration.alexa.music.entities.AlternateNames</class>
        <class>f18a14c09s.integration.alexa.music.entities.PopularityOverride</class>
        <class>f18a14c09s.integration.alexa.data.Locale</class>
        <class>f18a14c09s.integration.alexa.music.data.Art</class>
        <class>f18a14c09s.integration.alexa.music.data.ArtSource</class>
        <properties>
            <!--<property name="hibernate.show_sql" value="true"/>-->
            <!--<property name="hibernate.hbm2ddl.auto" value="create-drop"/>-->
            <property name="hibernate.hbm2ddl.auto" value="create"/>
        </properties>
    </persistence-unit>
</persistence>